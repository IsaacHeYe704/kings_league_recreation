---
import Container from "../components/Container.astro";
import Header from "../components/Header.astro";
import Table from "../components/Table.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Kings League infojobs">
    <main>
      <section class=" bg-[#b64c00]  grid grid-cols-5 " >
        <Container>

          <div class="col-span-2 ">
            <Table />
          </div>
        </Container>
      </section>
    </main>
</Layout>







<style>
  @font-face{
    font-family: 'Archivo';
    font-style: normal;
    font-weight: 400;
    font-display: swap;
    src: url('/fonts/archivo.woff') format('woff2');
  }
  @font-face{
    font-family: 'Archivo Black';
    font-style: normal;
    font-weight: 800;
    font-display: swap;
    src: url('/fonts/archivo_black.woff') format('woff2');
  }
  html, body {
    font-family: 'Archivo', system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    
  }
</style>